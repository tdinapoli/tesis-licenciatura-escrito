   ],
   "source": [
    "periods = 4\n",
    "x = np.linspace(0, periods, 1000)\n",
    "y = np.sin(2*np.pi * x)\n",
    "\n",
    "fig = plt.figure(figsize=(6,2))\n",
    "plt.plot(x, y, color=\"k\", linewidth=20)\n",
    "plt.ylim([-1.5, 1.5])\n",
    "plt.savefig(\"foton.svg\", dpi=300)\n",
    "plt.show()"
   ]
  }
